01006c4c <alt_sim_halt>:

/*
 * Routine called on exit.
 */
static ALT_ALWAYS_INLINE void alt_sim_halt(int exit_code)
{
 1006c4c:	defffd04 	addi	sp,sp,-12
 1006c50:	df000215 	stw	fp,8(sp)
 1006c54:	df000204 	addi	fp,sp,8
 1006c58:	e13fff15 	stw	r4,-4(fp)
  int r2 = exit_code;
 1006c5c:	e0bfff17 	ldw	r2,-4(fp)
 1006c60:	e0bffe15 	stw	r2,-8(fp)
  __asm__ volatile ("\n0:\n\taddi %0,%0, -1\n\tbgt %0,zero,0b" : : "r" (ALT_CPU_FREQ/100) ); /* Delay for >30ms */

  __asm__ volatile ("break 2" : : "D02"(r2), "D03"(r3) ALT_GMON_DATA );

#else /* !DEBUG_STUB */
  if (r2) {
 1006c64:	e0bffe17 	ldw	r2,-8(fp)
 1006c68:	1005003a 	cmpeq	r2,r2,zero
 1006c6c:	1000021e 	bne	r2,zero,1006c78 <alt_sim_halt+0x2c>
    ALT_SIM_FAIL();
