 1002dbc:	00000106 	br	1002dc4 <close+0x44>
 1002dc0:	e03fff15 	stw	zero,-4(fp)
 1002dc4:	e0bfff17 	ldw	r2,-4(fp)
 1002dc8:	e0bffb15 	stw	r2,-20(fp)

  if (fd)
