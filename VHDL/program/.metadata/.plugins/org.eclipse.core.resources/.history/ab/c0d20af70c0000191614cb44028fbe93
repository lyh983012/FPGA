 1004f60:	dfc00215 	stw	ra,8(sp)
 1004f64:	df000115 	stw	fp,4(sp)
 1004f68:	df000104 	addi	fp,sp,4
 1004f6c:	e13fff15 	stw	r4,-4(fp)
  /* Wait until the WIP bit goes low. */
